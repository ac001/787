        <format>%8.2f</format>
      </chunk>
    </chunks>
    <transformations>
      <transformation>
        <type>x-shift</type>
        <offset>-56</offset>
      </transformation>
      <transformation>
        <type>y-shift</type>
        <offset>28</offset>
      </transformation>
    </transformations>
  </layer>
  
  <layer>
    <name>nav2 frequencies</name>
