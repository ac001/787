        <property alias="../../../../../params/adf-standby"/>
        <format>%8.2f</format>
      </chunk>
    </chunks>
    <transformations>
      <transformation>
        <type>x-shift</type>
        <offset>-54</offset>
      </transformation>
      <transformation>
        <type>y-shift</type>
        <offset>-24</offset>
      </transformation>
    </transformations>
  </layer>
</layers>

 <actions>
   
   <action>
     <name>com frequency decrease (decimal part)</name>
     <button>0</button>
     <x>38</x>
     <y>74</y>
     <w>12</w>
     <h>16</h>
     <binding>
       <command>property-adjust</command>
       <property alias="../../../../params/comm1-standby"/>
       <mask>decimal</mask>
       <step>-0.025</step>
       <min>0.000</min>
       <max>1.000</max>
       <resolution>0.025</resolution>
       <wrap>true</wrap>
     </binding>
   </action>
   
