    </chunks>
    <transformations>
      <transformation>
        <type>x-shift</type>
        <offset>-56</offset>
      </transformation>
      <transformation>
        <type>y-shift</type>
        <offset>2</offset>
      </transformation>
    </transformations>
  </layer>

  <layer>
